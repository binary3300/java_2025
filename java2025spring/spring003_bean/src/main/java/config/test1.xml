<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Cat cat = new Cat() -->
	<bean id="cat" class="com.company.ioctest.Cat"/>
	
	<!-- Dog dog = new Dog() -->
	<bean id="dog" class="com.company.ioctest.Dog" />
	
	<!-- AnimalFarm farm = new AnimalFarm()
		animalFarm.aetName("sally");
		animalFarm.aetAni("cat");
	 -->
	<bean id="farm" class="com.company.ioctest.AnimalFarm">
		<property name="name" value="sally"></property>
		<property name="ani" value="cat"></property>
	</bean>

</beans>
